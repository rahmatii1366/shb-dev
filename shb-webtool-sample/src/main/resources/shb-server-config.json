{
  "server-type": "netty",
  "http-ip": "localhost",
  "http-port": "8000",
  "http-base-route": "",
  "rest-filters": [
  ],
  "asset-path": "D:/git-project/shb-dev/shb-client-sample/asset",
  "session":{
    "session-name": "SHADBARG_SESSION",
    "session-cache-size": "500",
    "session-expire-second": "100"
  },
  "cors": {
    "access-control-allow-origin": [
      "http://localhost:3000"
    ],
    "access-control-allow-headers": [
      "origin", "content-type", "accept", "authorization"
    ],
    "access-control-allow-credentials": "true",
    "access-control-allow-methods": [
      "GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD"
    ]
  }
}
